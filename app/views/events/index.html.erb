<h1>Listing Events</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Name</th>
      <th>Category</th>
      <th>Theme</th>
      <th>Venue</th>
      <th>Company</th>
      <th>Country</th>
      <th>Organizer</th>
      <th>Date</th>
      <th>Location</th>
      <!--<th>Link</th>-->
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= image_tag event.image.url(:thumb) %></td>
        <td><%= event.name %></td>
        
        <td><%= event.category %></td>
        <td><%= event.theme %></td>
        <td><%= event.venue %></td>
        <td><%= event.company %></td>
        <td><%= event.country %></td>

        <td><%= event.organizer %></td>
        <td><%= event.date %></td>
        <td><%= event.location %></td>
        <td><%#= event.link %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Delete', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

